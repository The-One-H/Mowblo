{
  "expo": {
    "name": "Mowblo",
    "slug": "Mowblo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Mowblo-Logo.jpg",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Mowblo-Logo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#2F80ED"
    },
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.schrodinger88.Mowblo",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.schrodinger88.Mowblo"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Mowblo-Logo.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "2493a427-45a2-4cc8-b985-1dd3a6cf42ab"
      }
    }
  }
}